.swiper-tab{
    width: 100%;
    border-bottom: 2rpx solid #ccc;
    text-align: center;
    height: 76rpx;
    line-height: 76rpx;
    display: flex;
    flex-flow: row;
    justify-content: space-between;
    background-color: white;
    font-size: 15px
}
.swiper-tab-item{
    width: 30%; 
    color:#434343;
}
.active{
    color:#edbe03;
    border-bottom: 4rpx solid #edbe03;
}
swiper{
    height: 100%;
    background: #f5f5f5;
    position:absolute;
    width: 100%;
    left: 0;
    transition: all 0.4s ease-in-out;
    -moz-transition: all 0.4s  ease-in-out; /* Firefox 4 */
    -webkit-transition: all 0.4s ease-in-out; /* Safari å’Œ Chrome */
    -o-transition: all 0.4s  ease-in-out; /* Opera */
}

